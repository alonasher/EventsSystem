{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "Topic": "events-topic"
  },
  "InfluxDB": {
    "Url": "http://localhost:8086",
    "Token": "my-super-secret-token",
    "Org": "my-org",
    "Bucket": "events-bucket"
  }
}